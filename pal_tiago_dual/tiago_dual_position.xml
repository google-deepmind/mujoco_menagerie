<mujoco model="tiago_dual">
  
  <include file="tiago_dual.xml"/>
  <actuator>
    <!-- Omni base actuators -->
    <velocity name="wheel_front_right_joint_velocity" joint="wheel_front_right_joint" ctrlrange="-5 5" kv="2000" forcerange="-400.0 400.0" />
    <velocity name="wheel_front_left_joint_velocity" joint="wheel_front_left_joint" ctrlrange="-5 5" kv="2000" forcerange="-400.0 400.0" />
    <velocity name="wheel_rear_right_joint_velocity" joint="wheel_rear_right_joint" ctrlrange="-5 5" kv="2000" forcerange="-400.0 400.0" />
    <velocity name="wheel_rear_left_joint_velocity" joint="wheel_rear_left_joint" ctrlrange="-5 5" kv="2000" forcerange="-400.0 400.0" />
    
    <!-- Torso actuator -->
    <position name="torso_lift_joint_position" joint="torso_lift_joint" kp="50000" inheritrange="0.98" forcerange="-500 500" dampratio="1.0"/>
    <!-- Head actuators -->
    <position name="head_1_joint_position" joint="head_1_joint" kp="20" inheritrange="0.93" forcerange="-5.197 5.197" dampratio="1.0"/>
    <position name="head_2_joint_position" joint="head_2_joint" kp="20" inheritrange="0.93" forcerange="-2.77 2.77" dampratio="1.0"/>

    <!-- Left arm actuators -->
    <position name="arm_left_1_joint_position" joint="arm_left_1_joint" inheritrange="1.0" kp="250" dampratio="1.0" forcerange="-53 53"/>
    <position name="arm_left_2_joint_position" joint="arm_left_2_joint" inheritrange="1.0" kp="1500" dampratio="1.0" forcerange="-53 53"/>
    <position name="arm_left_3_joint_position" joint="arm_left_3_joint" inheritrange="1.0" kp="600" dampratio="1.0" forcerange="-30 30"/>
    <position name="arm_left_4_joint_position" joint="arm_left_4_joint" inheritrange="1.0" kp="1000" dampratio="1.0" forcerange="-30 30"/>
    <position name="arm_left_5_joint_position" joint="arm_left_5_joint" inheritrange="1.0" kp="80" dampratio="1.0" forcerange="-3 3"/>
    <position name="arm_left_6_joint_position" joint="arm_left_6_joint" inheritrange="1.0" kp="80" dampratio="1.0" forcerange="-6.6 6.6"/>
    <position name="arm_left_7_joint_position" joint="arm_left_7_joint" inheritrange="1.0" kp="80" dampratio="1.0" forcerange="-6.6 6.6"/>
    <position name="gripper_left_left_finger_joint_position" joint="gripper_left_left_finger_joint" kp="500" inheritrange="0.999" dampratio="1.0" forcerange="-5.197 5.197"/>
    <position name="gripper_left_right_finger_joint_position" joint="gripper_left_right_finger_joint" kp="500" inheritrange="0.999" dampratio="1.0" forcerange="-5.197 5.197"/>

    <!-- Right arm actuators -->
    <position name="arm_right_1_joint_position" joint="arm_right_1_joint" inheritrange="1.0" kp="250" dampratio="1.0" forcerange="-53 53"/>
    <position name="arm_right_2_joint_position" joint="arm_right_2_joint" inheritrange="1.0" kp="1500" dampratio="1.0" forcerange="-53 53"/>
    <position name="arm_right_3_joint_position" joint="arm_right_3_joint" inheritrange="1.0" kp="600" dampratio="1.0" forcerange="-30 30"/>
    <position name="arm_right_4_joint_position" joint="arm_right_4_joint" inheritrange="1.0" kp="1000" dampratio="1.0" forcerange="-30 30"/>
    <position name="arm_right_5_joint_position" joint="arm_right_5_joint" inheritrange="1.0" kp="80" dampratio="1.0" forcerange="-3 3"/>
    <position name="arm_right_6_joint_position" joint="arm_right_6_joint" inheritrange="1.0" kp="80" dampratio="1.0" forcerange="-6.6 6.6"/>
    <position name="arm_right_7_joint_position" joint="arm_right_7_joint" inheritrange="1.0" kp="80" dampratio="1.0" forcerange="-6.6 6.6"/>
    <position name="gripper_right_left_finger_joint_position" joint="gripper_right_left_finger_joint" kp="500" inheritrange="0.999" dampratio="1.0" forcerange="-5.197 5.197"/>
    <position name="gripper_right_right_finger_joint_position" joint="gripper_right_right_finger_joint" kp="500" inheritrange="0.999" dampratio="1.0" forcerange="-5.197 5.197"/>
  </actuator>

</mujoco>
