<mujoco model="tiago">

  <include file="tiago.xml"/>
  <actuator>
    <!-- Arm actuators -->
    <motor name="arm_1_joint_motor" joint="arm_1_joint"/>
    <motor name="arm_2_joint_motor" joint="arm_2_joint"/>
    <motor name="arm_3_joint_motor" joint="arm_3_joint"/>
    <motor name="arm_4_joint_motor" joint="arm_4_joint"/>
    <!-- Wrist actuators -->
    <motor name="arm_5_joint_motor" joint="arm_5_joint" forcerange="-3 3"/>
    <motor name="arm_6_joint_motor" joint="arm_6_joint" forcerange="-6.6 6.6"/>
    <motor name="arm_7_joint_motor" joint="arm_7_joint"/>
    <!-- End effector actuators -->
    <position name="gripper_left_finger_position" joint="gripper_left_finger_joint" ctrlrange="0 0.045" forcerange="-64 64" kp="1000"/>
    <position name="gripper_right_finger_position" joint="gripper_right_finger_joint" ctrlrange="0 0.045" forcerange="-64 64" kp="1000"/>
    <!-- Head actuators -->
    <position name="head_1_joint_position" joint="head_1_joint" ctrlrange="-1.309 1.309" kp="20"/>
    <position name="head_2_joint_position" joint="head_2_joint" ctrlrange="-1.0472 0.785398" forcerange="-2.77 2.77" kp="20"/>
    <!-- Torso actuators -->
    <position name="torso_lift_joint_position" joint="torso_lift_joint" ctrlrange="0 0.35" kp="5000"/>
    <!-- Wheel actuators -->
    <velocity name="wheel_left_joint_vel" joint="wheel_left_joint" ctrlrange="-5 5" forcerange="-100 100" kv="100"/>
    <velocity name="wheel_right_joint_vel" joint="wheel_right_joint" ctrlrange="-5 5" forcerange="-100 100" kv="100"/>
  </actuator>

</mujoco>
